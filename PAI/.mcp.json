{
  "mcpServers": {
    "supabase-interlingo": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--project-ref",
        "anqfdvyhexpxdpgbkgmd"
      ],
      "env": {
        "SUPABASE_ACCESS_TOKEN": "${SUPABASE_ACCESS_TOKEN}"
      },
      "description": "Interlingo Supabase database access for jobs, interpreters, and communication management"
    },
    "github": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "ghcr.io/github/github-mcp-server@sha256:e8e5663bdac87864ffa72d3a6d7f97e3df1f1b244da329b6b6a5d2d25deecdbb"
      ],
      "description": "GitHub MCP server for repository management, issues, and pull requests"
    }
  }
}
